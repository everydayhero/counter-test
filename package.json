{
  "name": "counter-test-boilerplate",
  "version": "0.0.1",
  "description": "Javascript test boiler plate",
  "license": "MIT",
  "main": "server.js",
  "author": "EverydayHero",
  "scripts": {
    "setup": "npm install && npm run build && npm test",
    "build": "gulp build",
    "start": "node server.js",
    "test": "TEST_MODE=true && mocha `find . -name '*-test.js' -not -path '*node_modules*'`"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "blanket": "^1.1.6",
    "browserify": "~6.3.3",
    "uglify-js": "~2.5.0",
    "react": "~0.14.2",
    "chai": "^3.0.0",
    "glob": "^5.0.13",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-sass": "^2.1.0",
    "jsdom": "^3.1.2",
    "lodash": "^3.0.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "babelify": "^6.1.2"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "compression": "~1.0.8",
    "express": "~4.4.5",
    "morgan": "~1.1.1"
  }
}
